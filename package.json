{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Bowden Kelly",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "migrate": "node dist/migration.js",
    "migrate-ts": "npm run build-ts && npm run migrate",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "async": "^2.6.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.11.0",
    "compression": "^1.7.2",
    "connect-mongo": "^2.0.0",
    "constantinople": "^3.1.2",
    "cors": "^2.8.4",
    "deep-extend": "^0.6.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "fbgraph": "^1.4.4",
    "hoek": "^5.0.3",
    "is-my-json-valid": "^2.17.2",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.10",
    "lusca": "^1.6.0",
    "mongoose": "^5.1.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemailer": "^4.4.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "pug": "^2.0.3",
    "randomatic": "^3.0.0",
    "request": "^2.87.0",
    "sshpk": "^1.14.1",
    "stringstream": "0.0.6",
    "transform-props": "^1.0.1",
    "tunnel-agent": "^0.6.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.33",
    "@types/connect-mongo": "0.0.34",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/jest": "^21.1.10",
    "@types/jquery": "^3.3.2",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.109",
    "@types/lusca": "^1.5.0",
    "@types/mongodb": "^2.2.20",
    "@types/mongoose": "^4.7.36",
    "@types/morgan": "^1.7.32",
    "@types/multer": "^1.3.6",
    "@types/node": "^7.0.65",
    "@types/nodemailer": "^1.3.32",
    "@types/passport": "^0.4.5",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/passport-strategy": "^0.2.33",
    "@types/request": "^2.47.0",
    "@types/shelljs": "^0.7.9",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "^21.2.1",
    "nodemon": "^1.17.5",
    "shelljs": "^0.7.7",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
