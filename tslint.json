{
    "rules": {
        "class-name": true,
        "comment-format": [
            true
        ],
        "indent": [
            true,
            "spaces"
        ],
        "one-line": [
            true,
            "check-open-brace"
        ],
        "no-var-keyword": true,
        "semicolon": [
            true,
            "always",
            "ignore-bound-class-methods"
        ],
        "no-internal-module": true,
        "no-null-keyword": true,
        "prefer-const": true,
        "jsdoc-format": true
    }
}